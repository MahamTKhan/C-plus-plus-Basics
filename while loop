#include <iostream>
using namespace std;

int main()
{
    cout<<"WHILE LOOP IN C++"<<endl;
    int start;
    int end;
    cout<<"enter initial value"<<endl;
    cin>>start;
    cout<<"enter final value"<<endl;
    cin>>end;
    int i=start;
    
    while (i<=end){
        cout<<i<<endl;
        i++;
    }

    return 0;
}
