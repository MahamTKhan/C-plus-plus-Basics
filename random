#include <bits/stdc++.h>
using namespace std;

string palindrome(string s){
    string p=s;
    reverse(p.begin(),p.end());

    if (s==p);{
    return "true";
    }
  
    else{
        return "false";
    }
}

int main(){
    string s;
    cout<<"enter a string"<<endl;
    cin>>s;
    return 0;
}
