#include <iostream>
using namespace std;

int main(){
    int a;
    cout<<"enter value of a"<<endl;
    cin>>a;
switch(a)
{
  case 1:  	cout<<"this is case one!"<<endl;
        break;
  case 2: 	cout<<"this is the case two!"<<endl;
        break;
  case 3: 	cout<<"this is case three!"<<endl;
        break;
  default: 	cout<<"this is default"<<endl;
}
    return 0;
}
