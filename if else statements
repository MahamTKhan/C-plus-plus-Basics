#include <iostream>
using namespace std;
int main()
{
    cout<<"trying out IF-ELSE statements in C++"<<endl;

    int age;
    cout<<"Enter your age:"<<endl;
    cin>>age;

    if ((age<18) && (age>1)){
        cout<<"you are not eligible to vote";
    }else if (age==18){
        cout<<"you are eligible to vote";
    }else if (age<1){
        cout<<"you are not born yet";
    }else if (age>=60){
        cout<<"you are a senior citizen";
    }else{
        cout<<"age entered";
    }
    return 0;
}
